[{"name":"semi.min_h","formula":"nojac(semi.minsemi(sqrt(sqrt(1/emetric2))))","unit":"m","meaning":"Minimum element size","scope":"Global"},{"name":"semi.ngX","formula":"nX","unit":"1","meaning":"Exterior normal vector, X component","scope":"Boundaries 4, 9, 13, 18"},{"name":"semi.ngY","formula":"nY","unit":"1","meaning":"Exterior normal vector, Y component","scope":"Boundaries 4, 9, 13, 18"},{"name":"semi.ngZ","formula":"0","unit":"1","meaning":"Exterior normal vector, Z component","scope":"Boundaries 4, 9, 13, 18"},{"name":"semi.ngX","formula":"dnX","unit":"1","meaning":"Exterior normal vector, X component","scope":"Boundaries 1–3, 5, 7, 10, 12, 14, 16, 19, 21–22"},{"name":"semi.ngY","formula":"dnY","unit":"1","meaning":"Exterior normal vector, Y component","scope":"Boundaries 1–3, 5, 7, 10, 12, 14, 16, 19, 21–22"},{"name":"semi.ngZ","formula":"0","unit":"1","meaning":"Exterior normal vector, Z component","scope":"Boundaries 1–3, 5, 7, 10, 12, 14, 16, 19, 21–22"},{"name":"semi.dngX","formula":"dnX","unit":"1","meaning":"Down normal vector on mesh boundaries, X component","scope":"Global"},{"name":"semi.dngY","formula":"dnY","unit":"1","meaning":"Down normal vector on mesh boundaries, Y component","scope":"Global"},{"name":"semi.dngZ","formula":"0","unit":"1","meaning":"Down normal vector on mesh boundaries, Z component","scope":"Global"},{"name":"semi.ungX","formula":"unX","unit":"1","meaning":"Up normal vector on mesh boundaries, X component","scope":"Global"},{"name":"semi.ungY","formula":"unY","unit":"1","meaning":"Up normal vector on mesh boundaries, Y component","scope":"Global"},{"name":"semi.ungZ","formula":"0","unit":"1","meaning":"Up normal vector on mesh boundaries, Z component","scope":"Global"},{"name":"semi.rX","formula":"X","unit":"m","meaning":"Position vector, X component","scope":"Global"},{"name":"semi.rY","formula":"Y","unit":"m","meaning":"Position vector, Y component","scope":"Global"},{"name":"semi.rZ","formula":"0","unit":"m","meaning":"Position vector, Z component","scope":"Global"},{"name":"semi.dc_down","formula":"down( semi.rX - circumcenter(semi.rX[1/m])[m]) * semi.dngX + down(semi.rY -circumcenter( semi.rY[1/m] ) [m] ) * semi.dngY + down(semi.rZ - circumcenter(semi.rZ[1/m])[m]) * semi.dngZ","unit":"m","meaning":"Local boundary mesh distance, downside element","scope":"Global"},{"name":"semi.dc_up","formula":"up(semi.rX-circumcenter(semi.rX[1/m])[m])*semi.ungX+up(semi.rY-circumcenter(semi.rY[1/m])[m])*semi.ungY+up(semi.rZ-circumcenter(semi.rZ[1/m])[m])*semi.ungZ","unit":"m","meaning":"Local boundary mesh distance, upside element","scope":"Global"},{"name":"semi.global_flux_tol_cd","formula":"1.0E-4","unit":"1","meaning":"Global tolerance for finite volume fluxes (variable is replaced in subsequent semiconductor material model or charge conservation features)","scope":"Global"},{"name":"semi.dc_tol","formula":"semi.min_h*semi.global_flux_tol_cd","unit":"m","meaning":"Local boundary mesh distance tolerance","scope":"Global"},{"name":"semi.dc_n","formula":"if(abs(semi.dc_n_bnd)","unit":"m","meaning":"Local exterior boundary distance at domain edge, (numerical)","scope":"Boundaries 1–3, 5, 7, 10, 12, 14, 16, 19, 21–22"},{"name":"semi.dc_n_bnd","formula":"down(semi.rX-circumcenter(semi.rX[1/m])[m])*semi.ngX+down(semi.rY-circumcenter(semi.rY[1/m])[m])*semi.ngY+down(semi.rZ-circumcenter(semi.rZ[1/m])[m])*semi.ngZ","unit":"m","meaning":"Local exterior boundary distance at domain edge","scope":"Boundaries 1–3, 5, 7, 10, 12, 14, 16, 19, 21–22"},{"name":"semi.dc_down_bnd","formula":"if(abs(semi.dc_down)","unit":"m","meaning":"Local boundary mesh distance, downside element (used on boundaries)","scope":"Global"},{"name":"semi.dc_up_bnd","formula":"if(abs(semi.dc_up)","unit":"m","meaning":"Local boundary mesh distance, upside element (used on boundaries)","scope":"Global"},{"name":"semi.dc_tot","formula":"if(abs(semi.dc_down+semi.dc_up)","unit":"m","meaning":"Distance between circumcenters at interior boundary, (numerical)","scope":"Global"},{"name":"semi.delta_eta_tol","formula":"1.0E-9","unit":"1","meaning":"Tolerance for optimal average Gn Gp value","scope":"Global"},{"name":"semi.epsilonr_down","formula":"down(semi.epsilonrs)","unit":"1","meaning":"Equivalent relative permittivity, downside","scope":"Global"},{"name":"semi.epsilonr_up","formula":"up(semi.epsilonrs)","unit":"1","meaning":"Equivalent relative permittivity, upside","scope":"Global"},{"name":"semi.E_cd_denom","formula":"semi.epsilonr_down*semi.dc_up+semi.epsilonr_up*semi.dc_down","unit":"m","meaning":"Denominator for centered difference scheme","scope":"Global"},{"name":"semi.E_cd_denom_num","formula":"if(abs(semi.E_cd_denom)","unit":"m","meaning":"Numerical denominator for centered difference scheme","scope":"Global"},{"name":"semi.Dcd","formula":"epsilon0_const*semi.epsilonr_down*semi.epsilonr_up*(down(V)-up(V))/semi.E_cd_denom_num","unit":"C/m²","meaning":"Normal component of the electric displacement field","scope":"Global"},{"name":"semi.E_down","formula":"<div># semi.Dcd: Normal component of the electric displacement field</div><div>semi.Dcd / (epsilon0_const * semi.epsilonr_down)<br></div>","unit":"V/m","meaning":"Normal component of the electric field in the down direction","scope":"Global"},{"name":"semi.E_up","formula":"<div># semi.Dcd: Normal component of the electric displacement field<br></div>semi.Dcd / (epsilon0_const * semi.epsilonr_up)","unit":"V/m","meaning":"Normal component of the electric field in the up direction","scope":"Global"},{"name":"semi.D_bnd_down","formula":"epsilon0_const*semi.epsilonr_down*(down(V)-semi.nV)/semi.dc_down_bnd","unit":"C/m²","meaning":"Normal component of the electric displacement field in the down direction","scope":"Boundaries 1–5, 7, 9–10, 12–14, 16, 18–19, 21–22"},{"name":"semi.D_bnd_up","formula":"epsilon0_const*semi.epsilonr_up*(up(V)-semi.nV)/semi.dc_up_bnd","unit":"C/m²","meaning":"Normal component of the electric displacement field in the up direction","scope":"Boundaries 1–5, 7, 9–10, 12–14, 16, 18–19, 21–22"},{"name":"semi.EX","formula":"semi.DX/(semi.epsilonrs*epsilon0_const)","unit":"V/m","meaning":"Electric field, X component","scope":"Domains 1–2, 4–5, 7"},{"name":"semi.EY","formula":"semi.DY/(semi.epsilonrs*epsilon0_const)","unit":"V/m","meaning":"Electric field, Y component","scope":"Domains 1–2, 4–5, 7"},{"name":"semi.EZ","formula":"0","unit":"V/m","meaning":"Electric field, Z component","scope":"Domains 1–2, 4–5, 7"},{"name":"semi.T0","formula":"293.15[K]","unit":"K","meaning":"Reference temperature for energy levels","scope":"Domains 1–2, 4–5, 7"},{"name":"semi.epsilonrs","formula":"(semi.epsilonrXX+semi.epsilonrYY+semi.epsilonrZZ)/3","unit":"1","meaning":"Relative permittivity, scalar","scope":"Domains 1–2, 4–5, 7"},{"name":"semi.rho","formula":"0","unit":"C/m³","meaning":"Space charge density","scope":"undefined"},{"name":"semi.rho_s","formula":"0","unit":"C/m²","meaning":"Surface charge density","scope":"undefined"},{"name":"semi.nV","formula":"(semi.epsilonr_down*semi.dc_up*down(V)+semi.epsilonr_up*semi.dc_down*up(V)+semi.dc_down*semi.dc_up*semi.rho_s/epsilon0_const)/semi.E_cd_denom_num","unit":"V","meaning":"Electric potential at boundary","scope":"Boundaries 4, 9, 13, 18"},{"name":"semi.d","formula":"d_z","unit":"m","meaning":"Out-of-plane thickness","scope":"Domains 1–2, 4–5, 7"},{"name":"semi.gamma_n","formula":"exp(semi.log_FD_half(semi.eta_n)-semi.eta_n)","unit":"1","meaning":"Electron degeneracy factor","scope":"Domains 1–2, 4–5, 7"},{"name":"semi.Gn","formula":"exp(semi.log_FD_half(semi.eta_n)-semi.log_FD_minushalf(semi.eta_n))","unit":"1","meaning":"Electron degeneracy function","scope":"Domains 1–2, 4–5, 7"},{"name":"semi.Qthn","formula":"2*semi.Vth*semi.mun*exp(semi.log_FD_one(semi.eta_n)-semi.log_FD_zero(semi.eta_n))","unit":"m²/s","meaning":"Electron non-equilibrium contribution to the thermal diffusion coefficient","scope":"Domains 1–2, 4–5, 7"},{"name":"semi.logx_n","formula":"log(semi.N/semi.Nc)","unit":"1","meaning":"Log of N/Nc","scope":"Domains 1–2, 4–5, 7"},{"name":"semi.eta_n","formula":"if(semi.N/semi.Nc","unit":"1","meaning":"Inverse Fermi-Dirac function, electrons","scope":"Domains 1–2, 4–5, 7"},{"name":"semi.FD_half_etaeff_n","formula":"exp(semi.log_FD_half(semi.eta_n))","unit":"1","meaning":"Fermi-Dirac integral of order 1/2, eta n","scope":"Domains 1–2, 4–5, 7"},{"name":"semi.FD_one_etaeff_n","formula":"exp(semi.log_FD_one(semi.eta_n))","unit":"1","meaning":"Fermi-Dirac integral of order 1, eta n","scope":"Domains 1–2, 4–5, 7"},{"name":"semi.FD_minushalf_eta_n","formula":"exp(semi.log_FD_minushalf(semi.eta_n))","unit":"1","meaning":"Fermi-Dirac integral of order -1/2, eta n","scope":"Domains 1–2, 4–5, 7"},{"name":"semi.FD_zero_etaeff_n","formula":"exp(semi.log_FD_zero(semi.eta_n))","unit":"1","meaning":"Fermi-Dirac integral of order 0, eta n","scope":"Domains 1–2, 4–5, 7"},{"name":"semi.gamma_p","formula":"exp(semi.log_FD_half(semi.eta_p)-semi.eta_p)","unit":"1","meaning":"Hole degeneracy factor","scope":"Domains 1–2, 4–5, 7"},{"name":"semi.Gp","formula":"exp(semi.log_FD_half(semi.eta_p)-semi.log_FD_minushalf(semi.eta_p))","unit":"1","meaning":"Hole degeneracy function","scope":"Domains 1–2, 4–5, 7"},{"name":"semi.Qthp","formula":"2*semi.Vth*semi.mup*exp(semi.log_FD_one(semi.eta_p)-semi.log_FD_zero(semi.eta_p))","unit":"m²/s","meaning":"Hole non-equilibrium contribution to the thermal diffusion coefficient","scope":"Domains 1–2, 4–5, 7"},{"name":"semi.logx_p","formula":"log(semi.P/semi.Nv)","unit":"1","meaning":"Log of P/Nv","scope":"Domains 1–2, 4–5, 7"},{"name":"semi.eta_p","formula":"if(semi.P/semi.Nv","unit":"1","meaning":"Inverse Fermi-Dirac function, holes","scope":"Domains 1–2, 4–5, 7"},{"name":"semi.FD_half_etaeff_p","formula":"exp(semi.log_FD_half(semi.eta_p))","unit":"1","meaning":"Fermi-Dirac integral of order 1/2, eta p","scope":"Domains 1–2, 4–5, 7"},{"name":"semi.FD_one_etaeff_p","formula":"exp(semi.log_FD_one(semi.eta_p))","unit":"1","meaning":"Fermi-Dirac integral of order 1, eta p","scope":"Domains 1–2, 4–5, 7"},{"name":"semi.FD_minushalf_eta_p","formula":"exp(semi.log_FD_minushalf(semi.eta_p))","unit":"1","meaning":"Fermi-Dirac integral of order -1/2, eta p","scope":"Domains 1–2, 4–5, 7"},{"name":"semi.FD_zero_etaeff_p","formula":"exp(semi.log_FD_zero(semi.eta_p))","unit":"1","meaning":"Fermi-Dirac integral of order 0, eta p","scope":"Domains 1–2, 4–5, 7"},{"name":"semi.cp","formula":"1","unit":"1","meaning":"Interface continuation parameter","scope":"Domains 1–2, 4–5, 7"},{"name":"semi.cp_dtd","formula":"1","unit":"1","meaning":"Doping and trap density continuation parameter","scope":"Domains 1–2, 4–5, 7"},{"name":"semi.nX","formula":"nX","unit":"","meaning":"Normal vector, X component","scope":"Boundaries 4, 9, 13, 18"},{"name":"semi.nY","formula":"nY","unit":"","meaning":"Normal vector, Y component","scope":"Boundaries 4, 9, 13, 18"},{"name":"semi.nZ","formula":"0","unit":"","meaning":"Normal vector, Z component","scope":"Boundaries 4, 9, 13, 18"},{"name":"semi.nX","formula":"dnX","unit":"","meaning":"Normal vector, X component","scope":"Boundaries 1–3, 5, 7, 10, 12, 14, 16, 19, 21–22"},{"name":"semi.nY","formula":"dnY","unit":"","meaning":"Normal vector, Y component","scope":"Boundaries 1–3, 5, 7, 10, 12, 14, 16, 19, 21–22"},{"name":"semi.nZ","formula":"0","unit":"","meaning":"Normal vector, Z component","scope":"Boundaries 1–3, 5, 7, 10, 12, 14, 16, 19, 21–22"},{"name":"semi.nmeshX","formula":"nXmesh","unit":"","meaning":"Mesh normal vector, X component","scope":"Boundaries 4, 9, 13, 18"},{"name":"semi.nmeshY","formula":"nYmesh","unit":"","meaning":"Mesh normal vector, Y component","scope":"Boundaries 4, 9, 13, 18"},{"name":"semi.nmeshZ","formula":"0","unit":"","meaning":"Mesh normal vector, Z component","scope":"Boundaries 4, 9, 13, 18"},{"name":"semi.nmeshX","formula":"dnXmesh","unit":"","meaning":"Mesh normal vector, X component","scope":"Boundaries 1–3, 5, 7, 10, 12, 14, 16, 19, 21–22"},{"name":"semi.nmeshY","formula":"dnYmesh","unit":"","meaning":"Mesh normal vector, Y component","scope":"Boundaries 1–3, 5, 7, 10, 12, 14, 16, 19, 21–22"},{"name":"semi.nmeshZ","formula":"0","unit":"","meaning":"Mesh normal vector, Z component","scope":"Boundaries 1–3, 5, 7, 10, 12, 14, 16, 19, 21–22"},{"name":"semi.unmeshX","formula":"unXmesh","unit":"","meaning":"Mesh normal vector, upside, X component","scope":"Boundaries 1–5, 7, 9–10, 12–14, 16, 18–19, 21–22"},{"name":"semi.unmeshY","formula":"unYmesh","unit":"","meaning":"Mesh normal vector, upside, Y component","scope":"Boundaries 1–5, 7, 9–10, 12–14, 16, 18–19, 21–22"},{"name":"semi.unmeshZ","formula":"0","unit":"","meaning":"Mesh normal vector, upside, Z component","scope":"Boundaries 1–5, 7, 9–10, 12–14, 16, 18–19, 21–22"},{"name":"semi.dnmeshX","formula":"dnXmesh","unit":"","meaning":"Mesh normal vector, downside, X component","scope":"Boundaries 1–5, 7, 9–10, 12–14, 16, 18–19, 21–22"},{"name":"semi.dnmeshY","formula":"dnYmesh","unit":"","meaning":"Mesh normal vector, downside, Y component","scope":"Boundaries 1–5, 7, 9–10, 12–14, 16, 18–19, 21–22"},{"name":"semi.dnmeshZ","formula":"0","unit":"","meaning":"Mesh normal vector, downside, Z component","scope":"Boundaries 1–5, 7, 9–10, 12–14, 16, 18–19, 21–22"},{"name":"semi.Vref","formula":"semi.Vref_local","unit":"V","meaning":"Reference potential","scope":"Point 1"},{"name":"semi.Vref_global","formula":"semi.intsemi(semi.Vref)","unit":"V","meaning":"Reference potential, global","scope":"Global"}]